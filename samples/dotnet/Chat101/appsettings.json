{
    "Service":
    {
        "Type": "<AI-service-type>", // Valid options: "AzureOpenAI", "OpenAI"
        "AzureOpenAI":
        {
            "ChatModelDeploymentName": "<chat-model-deployment-name>",
            "Endpoint": "https://<fill-in>.openai.azure.com/"
            // "APIKey": Set with `dotnet user-secrets set "Service:AzureOpenAI:APIKey" "<my-azureopenai-apikey>"`
        },
        "OpenAI":
        {
            "ChatModelName": "<chat-model-name>"
            // "APIKey": Set with `dotnet user-secrets set "Service:OpenAI:APIKey" "<my-openai-apikey>"`
        }
    },
    "Application":
    {
        "UseContext": true
    }
  }
